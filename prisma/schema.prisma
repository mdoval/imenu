generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model etiquetatoproducto {
  A Int
  B Int

  @@unique([A, B], map: "_etiquetaToproducto_AB_unique")
  @@index([B], map: "_etiquetaToproducto_B_index")
  @@map("_etiquetatoproducto")
}

model etiqueta {
  id     Int    @id @default(autoincrement())
  nombre String
  barId  Int

  @@index([barId], map: "etiqueta_barId_fkey")
}

model imagen {
  id  Int    @id @default(autoincrement())
  url String
}

model producto {
  id          Int     @id @default(autoincrement())
  nombre      String
  descripcion String
  precio      Decimal
  imagen      String
  barId       Int

  @@index([barId], map: "producto_barId_fkey")
}

model usuario {
  id    Int    @id @default(autoincrement())
  email String @db.VarChar(255)
}

model bar {
  id          Int    @id @default(autoincrement())
  nombre      String @db.VarChar(255)
  logo        String @db.VarChar(255)
  descripcion String @db.Text
}

model bartousuario {
  A Int
  B Int

  @@unique([A, B], map: "_barTousuario_AB_unique")
  @@index([B], map: "_barTousuario_B_index")
  @@map("_bartousuario")
}
